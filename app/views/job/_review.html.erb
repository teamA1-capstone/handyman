<div id="review">
              
  <h5 style="font-weight:bold;">Review of Worker Performance on the Job</h5>
  <h6 style="font-weight:bold;"><%= @review.headline%></h6>

  <p><%= @review.body %></p>


  <p> Reliability: </p>


  <div id="reliability">
    <% i = 1 %>
    <% while i < 6%>
      <img src="<%= asset_path 'star-blue-outline.svg' %>" id='<%= i%>' class='star'/>
      <%i = i + 1%>
    <% end %>
  </div>

  <br>


  <p> Honesty: </p>


  <div id="honesty">
    <% while i < 11%>
      <img src="<%= asset_path 'star-blue-outline.svg' %>" id='<%= i%>' class='star'/>
      <%i = i + 1%>
    <% end %>
  </div>

  <br>


  <p> Skill: </p>


  <div id="skill" >
    <% while i < 16%>
      <img src="<%= asset_path 'star-blue-outline.svg' %>" id='<%= i%>' class='star'/>
      <%i = i + 1%>
    <% end %>
  </div>

  <br>


  <p> Focus: </p>

  <div class="focus">
    <% while i < 21%>
      <img src="<%= asset_path 'star-blue-outline.svg' %>" id='<%= i%>' class='star'/>
      <%i = i + 1%>
    <% end %>
  </div>

  <!-- there is a picture uploaded, just cant figure out how to display -->

</div>